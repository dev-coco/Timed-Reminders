{
    "manifest_version": 2,
    "name": "Timed Reminders",
    "description": "定时提醒。",
    "author": "dev-coco",
    "version": "1.0.1",
    "browser_action": {
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "*://*.youtube.com/*",
          "*://youtube.com/*"
        ],
        "js": ["jquery-3.4.1.min.js", "inject.js"]
      }
    ]  
}
